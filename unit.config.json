{
    "listeners": {
        "*:8000": {
            "pass": "applications/flaskapp"
        }
    },
    "applications": {
        "flaskapp": {
            "type": "python",
            "home": "/app",
            "path": "/app",
            "module": "web",
            "callable": "app",
            "processes": {
                "max": 4,
                "spare": 1
            }
        }
    }
}
